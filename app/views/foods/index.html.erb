<h3 class="card-title">Foods </h3>
<table>
    <tr>
      <th>Food</th>
      <th>Measurement unit</th>
      <th>Unit price</th>
      <th>Action</th>
    </tr>
    <% @foods.each do |food| %>
    <tr>
      <td><%= food.name%></td>
      <td><%= food.measurement_unit%></td>
      <td><%= food.price%></td>
      <td><%= button_to 'Delete', food_path(id: food), method: :delete%></td>
    </tr>
    <% end %>
  
</table>
<%= button_to 'Add new food', new_food_path, method: :get%>